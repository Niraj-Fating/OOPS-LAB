#include <iostream>
using namespace std;

int mult(int*,int*,int*);

int main()
{
    int a,b,c,d;
    cout<<"Enter 3 number:";
    cin>>a>>b>>c;
    d = mult(&a,&b,&c);
    cout<<"multiplication of number:"<<d;
    return 0;
}
int mult(int* x, int* y, int* z)
{
    int m;
    m=(*x)*(*y)*(*z);
    return m;
}

